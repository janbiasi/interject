{
	"name": "di",
	"version": "0.0.1",
	"description": "Flawless dependency injection for any platform",
	"main": "index.js",
	"scripts": {
		"test": "jest --watch",
		"test-only": "jest --verbose",
		"build": "webpack",
		"prettier": "prettier ./*.{json,js,ts} --write"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/janbiasi/flawless-di.git"
	},
	"keywords": [
		"di",
		"dependency",
		"injection",
		"dependency",
		"injection",
		"inject",
		"react",
		"vue"
	],
	"author": "Jan Biasi <biasijan@gmail.com> (http://janbiasi.ch)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/janbiasi/flawless-di/issues"
	},
	"homepage": "https://github.com/janbiasi/flawless-di#readme",
	"devDependencies": {
		"@types/jest": "23.3.2",
		"@types/node": "10.11.0",
		"html-webpack-plugin": "4.0.0-alpha.2",
		"jest": "23.6.0",
		"lerna": "^3.4.0",
		"prettier": "1.14.3",
		"reflect-metadata": "0.1.12",
		"ts-config-webpack-plugin": "0.4.3",
		"ts-jest": "23.10.1",
		"tslint": "5.11.0",
		"typescript": "3.0.3",
		"webpack": "4.20.1",
		"webpack-cli": "3.1.1"
	},
	"jest": {
		"roots": [
			"<rootDir>",
			"<rootDir>/test"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"collectCoverage": true,
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
		"collectCoverageFrom": ["**/*.ts"],
		"coveragePathIgnorePatterns": [
			"<rootDir>/test/__snapshots__",
			"<rootDir>/src/index.ts"
		],
		"moduleFileExtensions": [
			"js",
			"ts",
			"json"
		],
		"globals": {
			"ts-jest": {
				"diagnostics": {
					"ignoreCodes": [
						151001
					]
				}
			}
		}
	}
}
