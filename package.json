{
	"name": "@interject/common",
	"version": "0.0.1",
	"private": true,
	"description": "Flawless dependency injection for any platform",
	"main": "index.js",
	"scripts": {
		"cz": "git-cz",
		"clean": "./node_modules/.bin/run-s clean-*",
		"clean-lerna": "./node_modules/.bin/lerna clean",
		"clean-locks": "./node_modules/.bin/rimraf ./**/package-lock.json",
		"audit": "npm audit",
		"postinstall": "./node_modules/.bin/run-s bootstrap audit",
		"bootstrap": "./node_modules/.bin/lerna bootstrap --hoist",
		"test": "./node_modules/.bin/lerna run test",
		"test-only": "./node_modules/.bin/lerna run test-only",
		"build": "./node_modules/.bin/lerna run build",
		"prettier": "./node_modules/.bin/prettier ./**/*.{json,js,ts} --write"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/janbiasi/flawless-di.git"
	},
	"engines": {
		"npm": ">=6",
		"node": ">=8"
	},
	"keywords": ["di", "dependency", "injection", "dependency", "injection", "inject", "react", "vue"],
	"author": "Jan Biasi <biasijan@gmail.com> (http://janbiasi.ch)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/janbiasi/flawless-di/issues"
	},
	"homepage": "https://github.com/janbiasi/flawless-di#readme",
	"devDependencies": {
		"cz-conventional-changelog": "2.1.0",
		"npm-run-all": "4.1.3",
		"prettier": "1.14.3",
		"rimraf": "2.6.2",
		"lerna": "3.4.1"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
