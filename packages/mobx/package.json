{
	"name": "@interject/mobx",
	"version": "0.0.1",
	"description": "Use the interject framework together with Mobx",
	"scripts": {
		"test": "jest --watch",
		"test-only": "jest --verbose",
		"build": "webpack"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/janbiasi/flawless-di.git"
	},
	"keywords": ["di", "dependency", "injection", "dependency", "injection", "inject", "react", "react di"],
	"author": "Jan Biasi <biasijan@gmail.com> (http://janbiasi.ch)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/janbiasi/flawless-di/issues"
	},
	"devDependencies": {
		"@interject/config-typescript": "0.1.0",
		"@interject/config-webpack": "0.1.0",
		"@interject/core": "0.1.0",
		"@types/jest": "23.3.3",
		"ts-jest": "23.10.4",
		"jest": "23.6.0",
		"webpack-cli": "3.1.2",
		"typescript": "3.1.1"
	},
	"peerDependencies": {
		"@interject/core": ">=0.1.0"
	},
	"jest": {
		"roots": ["<rootDir>", "<rootDir>/test"],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"collectCoverage": true,
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
		"collectCoverageFrom": ["**/*.ts"],
		"coveragePathIgnorePatterns": ["<rootDir>/test/__snapshots__", "<rootDir>/src/index.ts"],
		"moduleFileExtensions": ["js", "ts", "json"],
		"globals": {
			"ts-jest": {
				"diagnostics": {
					"ignoreCodes": [151001]
				}
			}
		}
	}
}
